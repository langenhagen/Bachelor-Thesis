package home.stateReadersWriters;

import home.Home;

/**
 * This interface provides a method for loading state informations into a <i>Home</i>.
 * 
 * @author langenhagen
 * @version 20110925
 */
public interface StateReader {
	
	/**
	 * Loads the information according to its specialized implementation.
	 * @param home
	 * The home for which the information shall be loaded.
	 * @return
	 * Returns TRUE, if loading was successful, otherwise returns FALSE.
	 */
	public boolean load( Home home);
}
